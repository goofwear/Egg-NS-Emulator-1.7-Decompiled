<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3dp"/>
            <gradient android:startColor="#ffd1d1d1" android:endColor="#ffd1d1d1" android:angle="270" android:centerY="0.75" android:centerColor="#ffd1d1d1"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3dp"/>
                <gradient android:startColor="#ff90ee90" android:endColor="#ff90ee90" android:angle="270" android:centerY="0.75" android:centerColor="#ff90ee90"/>
            </shape>
        </clip>
    </item>
</layer-list>
